ℕ = σ {
	0   : ℕ
	1+  : ℕ → ℕ
}

+ = (λ {
	x : ℕ . (λ {
		((1+ : ℕ → ℕ) (y : ℕ)) : ℕ . ((1+ : ℕ → ℕ) (((((+ : ℕ → ℕ → ℕ) (x : ℕ)) : ℕ → ℕ) (y : ℕ)) : ℕ)) : ℕ
		0 : ℕ . x : ℕ
	}) : ℕ → ℕ
}) : ℕ → ℕ → ℕ

ℕ𝓁 = σ {
	∅   : ℕ𝓁
	++  : ℕ → ℕ𝓁 → ℕ𝓁
}

take = (λ {
	((1+ : ℕ → ℕ) (n : ℕ)) : ℕ . (λ {
		∅ : ℕ𝓁 . ∅ : ℕ𝓁
		((((++ : ℕ → ℕ𝓁 → ℕ𝓁) (x : ℕ)) : ℕ𝓁 → ℕ𝓁) (xs : ℕ𝓁)) : ℕ𝓁 . ((((++ : ℕ → ℕ𝓁 → ℕ𝓁) (x : ℕ)) : ℕ𝓁 → ℕ𝓁) (((((take : ℕ → ℕ𝓁 → ℕ𝓁) (n : ℕ)) : ℕ𝓁 → ℕ𝓁) (xs : ℕ𝓁)) : ℕ𝓁)) : ℕ𝓁
	}) : ℕ𝓁 → ℕ𝓁
	0 : ℕ . (λ {
		x : ℕ𝓁 . ∅ : ℕ𝓁
	}) : ℕ𝓁 → ℕ𝓁
}) : ℕ → ℕ𝓁 → ℕ𝓁
